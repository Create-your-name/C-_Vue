                .then((res) => {
                    this.formData = res.data;
                })
            },
            uniqueKey(array, key) {
                return array.reduce((result, item) => {
                    if (!result.find(e => e[key]===item[key])&&item[key]!=null) {
                        result.push(item);
                    }
                    return result;
                }, []);
            },
            QueryDepartList() {
                axios
                    .get("https://localhost:7155/DepartMent")
                    .then((res) => {
                        this.tableData = res.data;
                    })
